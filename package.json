{
  "name": "cryptones",
  "description": "webapp that converts cryptocurrency price data into audio with tone.js    ",
  "version": "2.0.0",
  "author": "Jordan Walsh",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jmwalsh91/cryptones"
  },
  "keywords": [
    "boilerplate",
    "react",
    "vite",
    "styled-components",
    "react-router",
    "storybook",
    "typescript"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "jest --passWithNoTests --maxWorkers=50%",
    "test:watch": "jest --maxWorkers=50% --watch",
    "test:ci": "jest --passWithNoTests --runInBand",
    "lint": "eslint src --ext ts,tsx --max-warnings=0",
    "format": "prettier src",
    "typecheck": "tsc",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "generate": "plop",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "app/**/*.{ts,tsx}": [
      "yarn tsc",
      "yarn eslint --fix",
      "yarn prettier --write"
    ]
  },
  "dependencies": {
    "@emotion/babel-plugin": "^11.9.2",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.6.2",
    "@mui/material": "^5.7.0",
    "@storybook/addon-actions": "6.4.22",
    "@storybook/addon-essentials": "6.4.22",
    "@storybook/addon-interactions": "6.4.22",
    "@storybook/addon-links": "6.4.22",
    "@storybook/builder-vite": "0.1.33",
    "@storybook/react": "6.4.22",
    "@storybook/testing-library": "0.0.11",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.1.1",
    "@testing-library/user-event": "14.1.1",
    "@tonaljs/tonal": "^4.6.5",
    "@types/jest": "27.4.1",
    "@types/react": "18.0.8",
    "@types/react-dom": "18.0.3",
    "@types/styled-components": "5.1.25",
    "@typescript-eslint/eslint-plugin": "5.19.0",
    "@typescript-eslint/parser": "5.19.0",
    "@vitejs/plugin-react": "1.3.2",
    "apexcharts": "^3.35.2",
    "axios": "^0.27.2",
    "eslint": "8.13.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.5.0",
    "husky": "7.0.4",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.5.1",
    "jest-styled-components": "7.0.8",
    "lint-staged": "12.4.1",
    "plop": "3.1.0",
    "prettier": "2.6.2",
    "react": "18.1.0",
    "react-apexcharts": "^1.4.0",
    "react-dom": "18.1.0",
    "react-error-boundary": "^3.1.4",
    "react-router-dom": "6.3.0",
    "styled-components": "5.3.5",
    "styled-media-query": "2.1.2",
    "swr": "^1.3.0",
    "tone": "^14.7.77",
    "ts-jest": "27.1.4",
    "ts-node": "10.7.0",
    "typescript": "4.6.4",
    "vite": "2.9.7",
    "vite-plugin-linter": "1.2.0",
    "vite-tsconfig-paths": "3.4.1"
  }
}
